int[] c = int[](10); 
c[0] = 1; 
c[1] = -3; 
c[2] = 4; 
c[3] = 2; 
c[4] = -9; 
c[5] = 12; 
c[6] = 5; 
c[7] = 3; 
c[8] = 1; 
c[9] = 5; 

int count = 0; 
int sum = 0; 
while(count < 10){ 
    if (count % 2 == 1 && c[count] > 0){ 
        sum = sum + c[count];
    } 
    count = count + 1;
} 
print('The sum is - ', sum);